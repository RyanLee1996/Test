<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ryan.hibernate">

    <class name="ryan.hibernate.EmployeeEntity" table="employee" schema="frame">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="name" column="name"/>
        <many-to-one name="departmentEntity" class="ryan.hibernate.DepartmentEntity">
            <column name="did"/>
        </many-to-one>
    </class>
</hibernate-mapping>