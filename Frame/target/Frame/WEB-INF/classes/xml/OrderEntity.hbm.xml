<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ryan.hibernate">

    <class name="ryan.hibernate.OrderEntity" table="orders" schema="frame">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="time" column="time"/>
        <property name="status" column="status"/>
        <set name="orderBySkuID" table="order_sku" cascade="save-update">
            <key>
                <column name="orderID"/>
            </key>
            <many-to-many class="ryan.hibernate.SkuEntity" column="skuID"/>
        </set>
    </class>
</hibernate-mapping>